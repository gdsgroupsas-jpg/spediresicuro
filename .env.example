# ============================================
# SUPABASE - Database
# ============================================
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# ============================================
# NEXTAUTH - Autenticazione
# ============================================
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_secret_here

# ============================================
# AI - Anthropic Claude
# ============================================
ANTHROPIC_API_KEY=your_anthropic_key

# ============================================
# ENCRYPTION - Crittografia
# ============================================
ENCRYPTION_KEY=your_encryption_key_32_chars

# ============================================
# OAUTH - Google (Opzionale)
# ============================================
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# ============================================
# OAUTH - GitHub (Opzionale)
# ============================================
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# ============================================
# DIAGNOSTICS - Sistema di Monitoring (Opzionale)
# ============================================
DIAGNOSTICS_TOKEN=your_diagnostics_token

# ============================================
# AUTOMATION SERVICE (Opzionale)
# ============================================
AUTOMATION_SERVICE_TOKEN=your_automation_service_token

# ============================================
# SMOKE TEST (Opzionale)
# ============================================
SUPABASE_TEST_EMAIL=smoke-test@spediresicuro.it
SUPABASE_TEST_PASSWORD=your_smoke_test_password

# ============================================
# CI/CD (Opzionale)
# ============================================
SUPABASE_SMOKE=0
