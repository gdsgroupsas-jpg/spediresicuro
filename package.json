    "dotenv": "^16.3.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^15.5.11",
    "happy-dom": "^20.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
