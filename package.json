    "resend": "^6.9.1",
    "sanitize-html": "^2.17.1",
    "sonner": "^2.0.7",
    "stripe": "^20.1.0",
    "swr": "^2.3.8",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
