{
  "name": "spediresicuro",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:monitor": "node scripts/error-monitor.js dev",
    "build": "next build",
    "build:monitor": "node scripts/error-monitor.js build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,scss}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css,scss}\"",
    "type-check": "tsc --noEmit",
    "doctor": "npm run check:errors",
    "check:errors": "node scripts/check-errors.js",
    "setup:check": "node scripts/setup-check.js",
    "setup:supabase": "ts-node --project tsconfig.scripts.json scripts/setup-supabase.ts",
    "verify:supabase": "ts-node --project tsconfig.scripts.json scripts/verify-supabase.ts",
    "check:table": "ts-node --project tsconfig.scripts.json scripts/check-table-structure.ts",
    "fix:schema": "ts-node --project tsconfig.scripts.json scripts/fix-schema.ts",
    "seed:geo": "ts-node --project tsconfig.scripts.json scripts/seed-geo.ts",
    "verify:config": "ts-node --project tsconfig.scripts.json scripts/verifica-config-locale.ts",
    "check:env": "ts-node --project tsconfig.scripts.json scripts/verifica-env-sicuro.ts",
    "check:env:simple": "node scripts/check-env-simple.js",
    "verify:users": "ts-node --project tsconfig.scripts.json scripts/verifica-crea-utenti-supabase.ts",
    "verify:reseller-wallet": "ts-node --project tsconfig.scripts.json scripts/verify-supabase-reseller-wallet.ts",
    "cleanup:test-users": "ts-node --project tsconfig.scripts.json scripts/cleanup-test-users.ts",
    "cleanup:test-users:execute": "ts-node --project tsconfig.scripts.json scripts/cleanup-test-users.ts --execute",
    "test:poste": "ts-node --project tsconfig.scripts.json scripts/test-poste-api.ts",
    "test:redis": "ts-node --project tsconfig.scripts.json scripts/test-redis-connection.ts",
    "create:smoke-test-user": "ts-node --project tsconfig.scripts.json scripts/create-smoke-test-user.ts",
    "test:supabase:smoke": "ts-node --project tsconfig.scripts.json scripts/smoke-test-supabase.ts",
    "test:supabase:smoke:dry": "ts-node --project tsconfig.scripts.json scripts/smoke-test-supabase.ts --dry-run",
    "test:supabase:smoke:ci": "node scripts/smoke-test-ci.js",
    "verify:schema": "ts-node --project tsconfig.scripts.json scripts/verify-shipments-schema.ts",
    "check:rls": "ts-node --project tsconfig.scripts.json scripts/check-supabase-rls.ts",
    "check:email-confirmations": "ts-node --project tsconfig.scripts.json scripts/check-email-confirmations.ts",
    "audit:security": "ts-node --project tsconfig.scripts.json scripts/audit-security.ts",
    "security:audit": "npm audit --audit-level=high",
    "security:scan": "npm audit --audit-level=high && npm run audit:security",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:debug": "npx playwright test --debug",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:auto-fix": "node scripts/auto-fix-playwright-test.js",
    "test:smoke:golden": "ts-node --project tsconfig.scripts.json scripts/smoke-test-golden-path.ts",
    "test:smoke:golden:dry": "ts-node --project tsconfig.scripts.json scripts/smoke-test-golden-path.ts --dry-run",
    "test:smoke:negative": "ts-node --project tsconfig.scripts.json scripts/smoke-test-negative-path.ts",
    "test:smoke:negative:dry": "ts-node --project tsconfig.scripts.json scripts/smoke-test-negative-path.ts --dry-run",
    "smoke:wallet": "npx tsx scripts/smoke-wallet.ts",
    "smoke:api": "node scripts/smoke-test-api.js",
    "smoke:api:local": "node scripts/smoke-test-api.js http://localhost:3000",
    "validate:api": "node scripts/validate-api-endpoints.js",
    "validate:api:local": "node scripts/validate-api-endpoints.js http://localhost:3000",
    "test": "vitest",
    "test:unit": "vitest run tests/unit --silent",
    "test:unit:verbose": "vitest run tests/unit",
    "test:ocr": "vitest run --config vitest.config.ocr.mts",
    "test:ocr:integration": "vitest run --config vitest.config.ocr-integration.mts",
    "test:integration": "vitest run tests/integration --silent",
    "test:integration:verbose": "vitest run tests/integration",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:vat": "vitest run tests/pricing/vat-utils.test.ts tests/pricing/vat-margin-zero-fix.test.ts tests/pricing/vat-semantics-comprehensive.test.ts",
    "test:vat:integration": "vitest run tests/integration/vat-semantics-flow.test.ts",
    "test:vat:regression": "vitest run tests/regression/vat-backward-compatibility.test.ts",
    "test:vat:all": "vitest run --run tests/pricing/vat tests/integration/vat-semantics-flow tests/regression/vat-backward-compatibility",
    "vercel": "vercel",
    "vercel:login": "vercel login",
    "vercel:deploy": "vercel --prod",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "generate:openapi": "tsx scripts/generate-openapi.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.js": [
      "node scripts/validate-syntax.js"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@eslint/eslintrc": "^3.3.3",
    "@google-cloud/vision": "^5.3.4",
